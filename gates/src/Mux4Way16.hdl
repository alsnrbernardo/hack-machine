/**
 * 4-way 16-bit multiplexor:
 * out = a if sel == 00
 *       b if sel == 01
 *       c if sel == 10
 *       d if sel == 11
 */

CHIP Mux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];

    PARTS:
    Xor   (a=sel[0], b=sel[1], out=xorSel);
    Mux16 (a=a, b=b, sel=xorSel, out=muxAOrB);
    Not   (in=xorSel, out=notXorSel);
    Mux16 (a=c, b=d, sel=notXorSel, out=muxCOrD);
    Mux16 (a=muxAOrB, b=muxCOrD, sel=sel[1], out=out);
}